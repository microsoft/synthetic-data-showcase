{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext"],
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true,
		"forceConsistentCasingInFileNames": true,
		"noErrorTruncation": true
	}
}
